@using System.Security.Claims
@{
    ViewBag.Title = "Contact";
}
<h2>View My Claims</h2>


<table class="table">
    <tr>
        <th>Claim Type</th>
        <th>Claim Value</th>
    </tr>
    @{
        var claimsPrincipal = User as ClaimsPrincipal;
        foreach (var claim in claimsPrincipal.Claims)
        {
            <tr>
                <td>@claim.Type </td>
                <td>@claim.Value</td>
            </tr>
        }
    }
</table>